    private void btnPrintActionPerformed(java.awt.event.ActionEvent evt) {                                         
        
        //Date and Time formatter
        DateTimeFormatter DateTimeFormat = DateTimeFormatter.ofPattern("dd.MM.yyyy HH:mm:ss");
        
        //Gets date & time of now
        LocalDateTime LocalDateTimeNow = LocalDateTime.now();
        
        //Gets today's date in a string
        String DateTimeToday = (DateTimeFormat.format(LocalDateTimeNow));

        sClan_1 = "";
        sClan_2 = "";
        sClan_3 = "";
        sClan_4 = "";
        sClan_5 = "";
        sClan_6 = "";
        sClan_7 = "";
        sClan_8 = "";
        sClan_9 = "";
        sClan_10 = "";
        sClan_11 = "";
        sClan_12 = "";
        sClan_13 = "";
        sPrint = sPrint = DateTimeToday;
        
        if (jCheckBox1.isSelected()){
            sClan_1 = jButton1.getText();
            sPrint = sPrint + ", " + sClan_1;
        }
        
        if (jCheckBox2.isSelected()){
            sClan_2 = jButton2.getText();
            sPrint = sPrint + ", " + sClan_2;
        }
        
        if (jCheckBox3.isSelected()){
            sClan_3 = jButton3.getText();
            sPrint = sPrint + ", " + sClan_3;
        }
        
        if (jCheckBox4.isSelected()){
            sClan_4 = jButton4.getText();
            sPrint = sPrint + ", " + sClan_4;
        }
        
        if (jCheckBox5.isSelected()){
            sClan_5 = jButton5.getText();
            sPrint = sPrint + ", " + sClan_5;
        }
        
        if (jCheckBox6.isSelected()){
            sClan_6 = jButton6.getText();
            sPrint = sPrint + ", " + sClan_6;
        }
        
        if (jCheckBox7.isSelected()){
            sClan_7 = jButton7.getText();
            sPrint = sPrint + ", " + sClan_7;
        }
        
        if (jCheckBox8.isSelected()){
            sClan_8 = jButton8.getText();
            sPrint = sPrint + ", " + sClan_8;
        }
        
        if (jCheckBox9.isSelected()){
            sClan_9 = jButton9.getText();
            sPrint = sPrint + ", " + sClan_9;
        }
        
        if (jCheckBox10.isSelected()){
            sClan_10 = jButton10.getText();
            sPrint = sPrint + ", " + sClan_10;
        }
        
        if (jCheckBox11.isSelected()){
            sClan_11 = jButton11.getText();
            sPrint = sPrint + ", " + sClan_11;
        }
        
        if (jCheckBox12.isSelected()){
            sClan_12 = jButton12.getText();
            sPrint = sPrint + ", " + sClan_12;
        }
        
        if (jCheckBox13.isSelected()){
            sClan_13 = jButton13.getText();
            sPrint = sPrint + ", " + sClan_13;
        }
        
        //Saves info to Text File
        try(
            FileWriter fw = new FileWriter("C:\\Users\\www.headoff.com\\Documents\\Text Document\\Teamspeak Clans.txt", true);
            BufferedWriter bw = new BufferedWriter(fw);
            PrintWriter out = new PrintWriter(bw)){
            out.println(sPrint);
        } catch ( IOException e ){
            // File writing/opening failed at some stage.
        }
